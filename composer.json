{
	"name": "flarum-lang/breton",
	"type": "flarum-extension",
	"description": "Breton language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/breton",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"br-locale",
		"breton-locale",
		"breton"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/breton/issues",
		"source": "https://github.com/flarum-lang/breton"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Breton",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "br",
			"title": "Breton"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
